# old code
if year % 4 == 0:
  print("yeah its a leap year!")
elif year % 100 == 0:
  print("Yeah its a leap year again!!")
else:
  print("that is not a leap year")
# simple leap year caculator

# new code

year = int(input("Which year do you want to check? "))
if year % 4 == 0:
  if year % 100 == 0:
    if year % 400 == 0:
      print("thats a leep year")
else:
  print("that is not a leap year")
